A = [-9, 3, 5, -2, 9, -7, 4, 8, 6]

max_producto = float('-inf')
par_numeros = ()

for i in range(len(A)):
    for j in range(i + 1, len(A)):
        producto = A[i] * A[j]
        if producto > max_producto:
            max_producto = producto
            par_numeros = (A[i], A[j])

print(f'El producto m√°ximo es {max_producto}, obtenido por el par {par_numeros}')
